{
    "name": "jensepas/laravel-permission-to-vuejs",
    "description": "Laravel Permission Package To Use it in VueJs",
    "type": "plugin",
    "require": {
        "spatie/laravel-permission": ">=2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelAndVueJS\\": "src"
        }
    },
	"extra": {
		"laravel": {
			"providers": [
				"LaravelAndVueJS\\LaravelAndVueJSServiceProvider"
			]
		}
	},
    "license": "MIT",
    "authors": [
        {
            "name": "jensepas",
            "email": "jensepas@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
